  <!-- ScrollViewer -->
  <Style x:Key="ScrollViewer" TargetType="{x:Type ContentControl}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ContentControl}">
          <Grid>

            <!-- bottom gradient -->
            <Rectangle x:Uid="Rectangle_21" x:Name="ContentBackgroundHighlightBottom" Width="Auto" Height="6" VerticalAlignment="Bottom">
              <Shape.Fill>
                <LinearGradientBrush x:Uid="LinearGradientBrush_15" StartPoint="0.5,0" EndPoint="0.5,1">
                  <GradientBrush.GradientStops>
                    <GradientStopCollection x:Uid="GradientStopCollection_17">
                      <GradientStop x:Uid="GradientStop_44" Color="{StaticResource BrightColor}" Offset="0" />
                      <GradientStop x:Uid="GradientStop_45" Color="{StaticResource BaseColor}" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>

            <!-- content -->
            <ContentPresenter x:Name="ContentPresenter" Margin="0,1,0,1" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="{TemplateBinding Content}" />

          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>


  <!-- Viewport -->
  <Style x:Key="Viewport" TargetType="{x:Type ContentControl}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ContentControl}">
          <Grid Background="White">

            <!-- top gradient -->
            <Rectangle x:Uid="Rectangle_22" x:Name="ContentBackgroundHighlightTop" Width="Auto" Height="12" VerticalAlignment="Top">
              <Shape.Fill>
                <LinearGradientBrush x:Uid="LinearGradientBrush_16" StartPoint="0.5,0" EndPoint="0.5,1">
                  <GradientBrush.GradientStops>
                    <GradientStopCollection x:Uid="GradientStopCollection_18">
                      <GradientStop x:Uid="GradientStop_46" Color="{StaticResource BaseColor}" Offset="0" />
                      <GradientStop x:Uid="GradientStop_47" Color="#00FFFFFF" Offset="1" />
                    </GradientStopCollection>
                  </GradientBrush.GradientStops>
                </LinearGradientBrush>
              </Shape.Fill>
            </Rectangle>

            <!-- content -->
            <ContentPresenter x:Name="ContentPresenter" Margin="0,3,0,1" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="{TemplateBinding Content}" />

          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

